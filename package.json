{
  "name": "parcel-bootstrap",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.6.5",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.23.0",
    "spuit": "^0.1.2"
  },
  "scripts": {
    "dev": "yarn clean && parcel ./src/index.html --out-file ./dist/index.html -p 8888 --open",
