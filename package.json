    "build": "yarn clean && parcel build ./src/index.html",
    "clean": "rm -r dist/ .cache/"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.14.7"
  }
